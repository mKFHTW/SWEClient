<Window x:Class="SWEClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="608" Width="649">
    <Grid>
        <GroupBox Header="Suchfilter" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="166" Width="621">
            <Grid x:Name="gridSuchfilter" HorizontalAlignment="Left" Height="144" VerticalAlignment="Top" Width="611" Margin="0,0,-2,0">
                <Label x:Name="lbVorname" Content="Vorname" HorizontalAlignment="Left" Margin="10,19,0,0" VerticalAlignment="Top"/>
                <Label x:Name="lbNachname" Content="Nachname" HorizontalAlignment="Left" Margin="10,60,0,0" VerticalAlignment="Top"/>
                <TextBox x:Name="tBoxVorname" HorizontalAlignment="Left" Height="23" Margin="123,23,0,0" TextWrapping="Wrap" Text="Bitte Teile des Vornamens eingeben .." VerticalAlignment="Top" Width="467"/>
                <TextBox x:Name="tBoxNachname" HorizontalAlignment="Left" Height="23" Margin="123,64,0,0" TextWrapping="Wrap" Text="Bitte Teile des Nachnamens eingeben .." VerticalAlignment="Top" Width="467"/>
                <Button x:Name="btnSuchfilterSuche" Content="Suche" HorizontalAlignment="Left" Margin="515,114,0,0" VerticalAlignment="Top" Width="75"/>
            </Grid>
        </GroupBox>

    </Grid>
</Window>
